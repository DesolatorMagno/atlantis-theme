{
    "name": "desolatormagno/atlantis-theme",
    "description": "Paquete basado en el tema gratuito atlantis",
    "keywords": ["themes", "laravel", "blade", "desolatormagno", "atlantis-theme"],
    "license": "MIT",
    "homepage": "https://github.com/DesolatorMagno/atlantis-theme",
    "authors": [
        {
            "name": "Reny Ramos",
            "email": "renyramosgarcia@gmail.com"
        }
    ],    
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*|5.8.*",
        "monolog/monolog": "1.23.*|1.24.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.6.*|3.7.*|3.8.*"
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DesolatorMagno\\AtlantisTheme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DesolatorMagno\\AtlantisTheme\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DesolatorMagno\\AtlantisTheme\\AtlantisThemeServiceProvider"
            ]
        }
    }
}
